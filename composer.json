{
    "name": "esplin/magento2-scripts",
    "description": "Helpful Magento2 scripts",
    "type": "magento2-component",
    "version": "1.0.0",
    "license": [
        "OSL-3.0",
        "AFL-3.0"
    ],
    "extra": {
        "map": [
            [
                "dist/bin/mage-clean",
                "bin/mage-clean"
            ],
            [
                "dist/bin/mage-init",
                "bin/mage-init"
            ],
            [
                "dist/bin/mage-refresh-composer",
                "bin/mage-refresh-composer"
            ]
        ],
        "chmod": [
            {
                "mask": "0755",
                "path": "bin/mage-clean"
            },
            {
                "mask": "0755",
                "path": "bin/mage-init"
            },
            {
                "mask": "0755",
                "path": "bin/mage-refresh-composer"
            }
        ]
    }
}
